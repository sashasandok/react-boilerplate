{
  "extends": "standard",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 9,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "plugins": ["import", "node", "promise", "standard"],
  "globals": {
    "document": false,
    "navigator": false,
    "window": false,
    "fetch": false
  },
  "rules": {
    "no-unused-vars": "off"
  }
}
